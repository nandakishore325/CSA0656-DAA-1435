#include <stdio.h>

void printPascal(int n) {
    for (int line = 0; line < n; line++) {
        int val = 1;
        for (int i = 0; i <= line; i++) {
            printf("%d ", val);
            val = val * (line - i) / (i + 1);
        }
        printf("\n");
    }
}

int main() {
    int n;
    printf("Y.Nanda Kishore reddy-192211435\n");
    printf("Enter N: ");
    scanf("%d",&n);
    printPascal(n);
    return 0;
}
